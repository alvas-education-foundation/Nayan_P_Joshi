   "source": [
    "def MagicNo(n): \n",
    "\n",
    "\tpow = 1\n",
    "\tanswer = 0\n",
    "\n",
    "\n",
    "\twhile (n): \n",
    "\n",
    "\t\tpow = pow*5\n",
    "\n",
    "\t\t# If last bit of n is set \n",
    "\t\tif (n & 1): \n",
    "\t\t\tanswer += pow\n",
    "\n",
    "\t\t# proceed to next bit \n",
    "\t\tn >>= 1 # or n = n/2 \n",
    "\t\n",
    "\treturn answer \n",
    "\n",
    "\n",
    "\n",
    "n = 5\n",
    "print(\"nth magic number is\", MagicNo(n)) \n",
    "\n"
   ]
