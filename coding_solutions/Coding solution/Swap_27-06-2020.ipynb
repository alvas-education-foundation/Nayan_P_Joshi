   "source": [
    "def swapList(newList): \n",
    "    size = len(newList) \n",
    " \n",
    "    temp = newList[0] \n",
    "    newList[0] = newList[size - 1] \n",
    "    newList[size - 1] = temp \n",
    "    return newList \n",
    "\n",
    "newList = [12, 35, 9, 56, 24] \n",
    "\n",
    "print(swapList(newList)) \n"
   ]
  }
